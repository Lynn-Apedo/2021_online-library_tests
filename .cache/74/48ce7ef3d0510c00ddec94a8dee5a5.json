tus === 201) {\n        history.push(\"/login\");\n        console.log(\"200 OK\");\n      }\n    } catch (error) {\n      console.log(\"connection failed\");\n      console.error(error.message);\n    }\n  };\n  return (\n    <div>\n      <Ban />\n      <h1>Register page !</h1>\n      <form onSubmit={handleSubmit}>\n        <Inputgroup type=\"text\" name=\"firstname\" value={handleChange} />\n        <Inputgroup type=\"text\" name=\"lastname\" value={handleChange} />\n        <Inputgroup type=\"email\" name=\"email\" value={handleChange} />\n        <Inputgroup\n          type=\"password\"\n          name=\"password\"\n          minlength=\"8\"\n          value={handleChange}\n        />\n        <Button name=\"Register\" />\n      </form>\n    </div>\n  );\n};\n\nexport default Register;\n"},"lineCount":null}},"error":null,"hash":"8ebed6d0529501f3597617462eec246c","cacheData":{"env":{}}}