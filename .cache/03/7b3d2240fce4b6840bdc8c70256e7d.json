},"source":"js/pages/login.js","original":{"line":10,"column":0}},{"name":"Login","generated":{"line":86,"column":15},"source":"js/pages/login.js","original":{"line":65,"column":15}}],"sources":{"js/pages/login.js":"import React, { useState } from 'react';\nimport api from '../../utils/api';\n\nimport Inputgroup from '../components/inputgroup';\nimport Button from '../components/button';\nimport { useHistory } from 'react-router-dom';\nimport { useDispatch } from 'react-redux';\nimport Ban from '../components/ban';\n\nconst Login = () => {\n  let history = useHistory();\n  let dispatch = useDispatch();\n  const [email, setEmail] = useState('');\n  const [password, setPassword] = useState('');\n\n  const [errors, setErrors] = useState(null);\n\n  const handleChange = (inputValue, inputName) => {\n    if (inputName === 'email') setEmail(inputValue);\n    if (inputName === 'password') setPassword(inputValue);\n  };\n\n  const handleSubmit = async (e) => {\n    e.preventDefault();\n    setErrors(null);\n    const body = {\n      email,\n      password,\n    };\n\n    try {\n      const result = await api.post('/admin/authenticate', body);\n      dispatch({ type: 'USER_SET', payload: result.data });\n      history.push('/dashboard');\n    } catch (error) {\n      console.log('connection failed');\n      setErrors(error.response?.data?.message);\n      dispatch({ type: 'USER_RESET' });\n      // history.push('/kiwi');\n      console.error(error.message);\n    }\n  };\n\n  return (\n    <div>\n      <Ban />\n      {errors && <p>{errors}</p>}\n      <div class=\"container_form-container\">\n        <h1>Login</h1>\n        <form onSubmit={handleSubmit}>\n          <Inputgroup type=\"email\" name=\"email\" value={handleChange} />\n          <Inputgroup\n            type=\"password\"\n            name=\"password\"\n            minlength=\"1\"\n            value={handleChange}\n          />\n          <Button name=\"Me connecter\" />\n        </form>\n      </div>\n    </div>\n  );\n};\n\nexport default Login;\n"},"lineCount":null}},"error":null,"hash":"6cc1bff040d0809e873adbe6db45bf29","cacheData":{"env":{}}}